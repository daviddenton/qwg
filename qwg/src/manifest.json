{
    "name":            "Qwg",
    "description":     "Reprogramme the Chrome Omnibox",
    "version":         "1.0",
    "manifest_version":2,
    "background":      {
        "scripts":[
            "lib/underscore/js/underscore.string.min.js",
            "lib/underscore/js/underscore-min.js",
            "lib/underscore/js/underscore.string.min.js",
            "bootstrap.js",
            "DefaultData.js",
            "ChromeStorage.js",
            "Qwg.js",
            "background.js"]
    },
    "icons":           {
        "16": "icon.png",
        "48": "icon.png",
        "128":"icon.png"
    },
    "omnibox":         {
        "keyword":"qwg"
    },
    "options_page":    "options.html",
    "permissions":     [
        "contextMenus", "storage", "tabs"
    ],
    "update_url":      "http://github.com/daviddenton/qwg/artifacts/update.xml"
}