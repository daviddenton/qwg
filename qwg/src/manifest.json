{
    "name":            "Qwg",
    "description":     "Reprogramme the Chrome Omnibox",
    "version":         "2.0",
    "manifest_version":2,
    "background":      {
        "page":"background.html"
    },
    "icons":           {
        "16": "icon.png",
        "48": "icon.png",
        "128":"icon.png"
    },
    "omnibox":         {
        "keyword":"qwg"
    },
    "options_page":    "options.html",
    "permissions": [
        "contextMenus", "storage", "tabs", "management"
    ],
    "sandbox":         {
        "pages":[ "sandbox.html"]
    }
}