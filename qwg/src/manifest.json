{
    "name":            "Qwg",
    "description":     "Reprogramme the Chrome Omnibox",
    "version":         "1.2",
    "manifest_version":2,
    "background":      {
        "page":"background.html"
    },
    "icons":           {
        "16": "icon.png",
        "48": "icon.png",
        "128":"icon.png"
    },
    "omnibox":         {
        "keyword":"qwg"
    },
    "options_page":    "options.html",
    "permissions":     [
        "contextMenus", "storage", "tabs"
    ],
    "sandbox":         {
        "pages":[ "sandbox.html"]
    },
    "update_url":      "http://github.com/daviddenton/qwg/update.xml"
}